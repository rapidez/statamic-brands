{
    "name": "rapidez/statamic-brands",
    "require": {
        "statamic/cms": "^3.3",
        "rapidez/statamic": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\StatamicBrands\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Brands",
            "description": "Statamic Brands addon"
        },
        "laravel": {
            "providers": [
                "Rapidez\\StatamicBrands\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
